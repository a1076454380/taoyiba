	<script type="text/javascript">
		$(function() {
			var gallery = mui('#myslide');
			gallery.slider({
				interval: 3000 //自动轮播周期，若为0则不自动播放，默认为0；
			});

			function plusReady() {
				// 弹出系统等待对话框
				var w = plus.nativeUI.showWaiting("数据加载中...");
			}
			if(window.plus) {
				plusReady();
			} else {
				document.addEventListener("plusready", plusReady, false);
			}
			setTimeout(function() {
				plus.nativeUI.closeWaiting()
			}, 1000)
			$('.bottom-tab').click(function() {
				opennew($(this).attr('data-target'))
			})

			function opennew(target) {
				mui.openWindow({
					url: '' + target + '.html',
					id: '' + target + ''
				})
			}
			$('#more').click(function() {
				$('.alert').css('top', '0')
			})
			$('.alert').click(function() {
				$('.alert').css('top', '-100%')
			})
			$('.video img').click(function() {
				$(this).prev().click()
			})
			$('video').click(function() {
				$('.myvideo video').attr('src', $(this).attr('src'))
				$('.myvideo').show()
			})
			$('.myvideo').click(function() {
				$('.myvideo').hide()
				$('.myvideo video').attr('src', '')
			})
			$('.alert-group').click(function(){
				opennew('goodslist')
			})
		})
	</script>

